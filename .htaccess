<IfModule mod_expires.c>
# Enable expirations
ExpiresActive On
# Default directive
ExpiresDefault "access plus 1 month"
# Images
ExpiresByType /img/profilepic.jpg  "access plus 1 month"
ExpiresByType /views/images/pizzeria.jpg "access plus 1 month"
# CSS
ExpiresByType css/print.css "access plus 1 month"
ExpiresByType /css/style.css  "access plus 1 month"
# Javascript
ExpiresByType /js/perfmatters.js "access plus 1 year"
</IfModule>
